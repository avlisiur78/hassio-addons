{
    "name": "RPi RF Receiver",
    "version": "2.0",
    "slug": "rfreceiver",
    "description": "RPi RF Receiver",
    "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
    "url": "https://github.com/avlisiur78/hassio-addons/tree/master/hassio-rpi-rf",
    "startup": "system",
    "boot": "auto",
    "hassio_api": true,
    "homeassistant_api": true,
    "host_network": true,	
    "devices": ["/dev/mem"],
    "privileged": ["SYS_RAWIO"],
    "gpio": "true",
    "apparmor": "false",
    "options": {
      "code": "/share/rpi-rf_receive.py",
      "requirements": ["rpi-rf"],
      "clean": true
    },
    "schema": {
      "code": "str",
      "requirements": ["str"],
      "clean": "bool?"
    },
    "map": ["share"]
}
